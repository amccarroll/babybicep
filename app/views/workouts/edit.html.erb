<h1>Edit Workout</h1>

<%= form_for :workout, url: workout_path(@workout), method: :patch do |f| %>
  <p>
    <%= f.label :trainer %><br>
    <%= f.text_field :trainer %>
  </p>
 
  <p>
    <%= f.label :intensity %><br>
    <%= f.text_field :intensity %>
  </p>

    <p>
    <%= f.label :program %><br>
    <%= f.text_area :program %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
